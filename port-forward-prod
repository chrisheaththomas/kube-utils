APP=`pwd | sed 's/^.*piano-//'`
FIRST_POD=`source getpods-prod | sed '2s/\(^'"$APP"'[a-z0-9-]*\)\(.*$\)/\1/' | sed 1d | sed 2d | sed 2d | sed 2d | sed 2d | sed 2d | sed 2d | sed 2d | sed 2d | sed 2d | sed 2d | sed 2d | sed 2d | sed 2d | sed 2d | sed 2d`
while true;do
kubeprod port-forward $FIRST_POD 9999:8080; done

